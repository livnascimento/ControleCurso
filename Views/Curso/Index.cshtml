@{
    ViewData["Title"] = "Adicionar Curso";
}

<form>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Título do curso</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
